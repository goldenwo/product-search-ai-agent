{
	"name": "BestBuy",
	"default_api_url": "https://api.bestbuy.com/v1/products",
	"allowed_params": ["categoryId", "price.min", "price.max"],
	"field_mapping": {
		"id": "sku",
		"title": "name",
		"price": "salePrice",
		"url": "productUrl",
		"description": "description",
		"category": "category",
		"brand": "brand",
		"image_url": "imageUrl"
	},
	"api_version": "v1",
	"rate_limit": {
		"requests_per_second": 3
	},
	"timeout": 5,
	"required_headers": {
		"apiKey": "ENV:BESTBUY_API_KEY"
	}
}
